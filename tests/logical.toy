fn test_logic(a, b, c) {
    if a && b || c {
        "pass"
    } else {
        "fail"
    }
}

fn test_negation(a, b) {
    if !a && b {
        "pass"
    } else {
        "fail"
    }
}

// true && false || true -> false || true -> true
test_logic(true, false, true) + " " + test_negation(false, true)
